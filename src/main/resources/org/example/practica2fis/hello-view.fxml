<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.shape.Line?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.practica2fis.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <ComboBox fx:id="cmb_tipo_persona"/>
    <?import javafx.scene.layout.GridPane?>
    <?import javafx.scene.shape.Line?>

    <GridPane hgap="5" vgap="5">
        <Line startX="100" startY="0" endX="100" endY="300" />
        <Label text="A1" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Label text="A2" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label text="B1" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Line startX="100" startY="50" endX="100" endY="50" />
        <Label text="B2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="C1" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <Label text="C2" GridPane.columnIndex="2" GridPane.rowIndex="1" />
    </GridPane>
</VBox>
